{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "jsx": "react",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "resolveJsonModule": true,
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ]
    }
  }
}